{
  "name": "studynotboring",
  "version": "1.1.0",
  "description": "A note-taking app with AI features and game-inspired theme",
  "main": "script.js",
  "type": "module",
  "scripts": {
    "start": "npx serve .",
    "build:css": "npx tailwindcss -i ./src/index.css -o ./dist/output.css --watch",
    "build": "npx tailwindcss -i ./src/index.css -o ./dist/output.css --minify && node build-css.cjs",
    "build:bundle": "node build-css.cjs",
    "test:tts": "node test-voice.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-icons": "^1.3.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "wav": "^1.0.2"
  },
  "keywords": [
    "notes",
    "ai",
    "education",
    "game"
  ],
  "author": "StudyNotBoring",
  "license": "MIT",
  "devDependencies": {
    "tailwindcss": "^4.1.12"
  }
}